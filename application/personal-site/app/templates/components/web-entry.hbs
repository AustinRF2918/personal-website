<div class="web-entry">
    <div class="centerable-half"><a class="web-entry-title" href="#">
        <h3 class="web-entry-title">{{title}}</h3></a>
        {{clickable-image imgSrc=this.imgSource action='toWebEntry'}}
        <h5 class="web-entry-genre">{{genre}}</h5>
        <h5 class="web-entry-date">{{date}}</h5>
    </div>
    <hr class="material-break"/>
</div>
